package com.example.tests;

import org.testng.annotations.Test;

public class ContactModificationTests extends TestBase {

	
@Test
	
	
public void testValidContactCanBeCreated() throws Exception {
ContactObject modifiedContact = new ContactObject("FirstNameTextModified", "LastNameTextModified", "addressTextModified", "0987654321", "0987654321", "workTextModified", "emailModified@mail.mail", "email2Modified@mail.mail", "1992", "address2TextModified", "0987654321", 5, 4, 0);	
	
app.getContactHelper().ModifyContact(modifiedContact);

}

}