<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
 <class name="com.example.fw.ContactObject" table="addressbook">
 <id name="id" column="id" ></id>
 <property name="ContactFirstName" column="firstname" />
 <property name="ContactLastName" column="lastname" />
 <property name="ContactEmail" column="email" />
 <property name="ContactHomePhone" column="home" />
 </class>
</hibernate-mapping>

